class Solution:
    def findTriplet(self, arr):
        n = len(arr)
        ind = set(arr)
        for i in range(n):
            for j in range(i+1, n):
                if arr[i] + arr[j] in ind:
                    return True
        return False


ğˆğ§ğ­ğğ«ğ¯ğ¢ğğ°ğğ¢ğ­ : ğ‘ğ¨ğ¦ğšğ§ ğ“ğ¨ ğˆğ§ğ­ğğ ğğ«
class Solution:
    # @param A : string
    # @return an integer
    def romanToInt(self, A):
        # x = "IVXLCDM"
        d = {
            "I":1,
            "V":5,
            "X":10,
            "L":50,
            "C":100,
            "D":500,
            "M":1000,
        }
        # print(d)
        ans = 0
        for i in range(len(A)-1):
            if d[A[i]] < d[A[i+1]]:
                ans -= d[A[i]]
            else:
                ans += d[A[i]]
        ans += d[A[len(A)-1]]
        return ans
            
