for _ in range(int(input())):
    n, k = map(int, input().split())
    A = list(input().split())
    for i in range(n):
        A[i] = int(A[i])
    res = ""
    for i in range(n):
        if (A[i] <= k):
            res += "1"
            k -= A[i]
        else:
            res += '0'
    print(res)


