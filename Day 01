t = int(input())
for _ in range(t):
    n = int(input())
    arr = list(map(int, input().split())) 
    if n == 2 and (arr[1] - arr[0] > 1 or arr[1] - arr[0] < -1):
        print("YES")
    else:
        print("NO")
