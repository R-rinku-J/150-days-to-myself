class Solution:
    #Function to convert a binary tree into its mirror tree.
    def mirror(self,root):
        # Code here
        if root is None:
            return None
        left_side = self.mirror(root.left)
        right_side = self.mirror(root.right)
        root.left = right_side
        root.right = left_side
        return root


