class Solution:
    def xorQueries(self, arr: List[int], queries: List[List[int]]) -> List[int]:
        res = []
        prefix = [0]
        for i in arr:
            prefix.append(prefix[-1] ^ i)
        for a, b in queries:
            res.append(prefix[b+1] ^ prefix[a])
        return res


