class Solution:
    def findTwoElement( self,arr): 
        # code here
        res = []
        for i in arr:
            index = abs(i) - 1
            if arr[index] < 0:
                res.append(abs(i))
            else:
                arr[index] *= -1
        for i, e in enumerate(arr):
            if e > 0:
                res.append(i+1)
        return res


