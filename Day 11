class Solution:
    def minExtraChar(self, s: str, dictionary: List[str]) -> int:
        
        dictionary = set(dictionary)
        max_len, n = len(max(dictionary, key = len)), len(s)
        dp = [0]*(n+1)
        for i, _ in enumerate(s, 1):
            dp[i] = dp[i-1] + 1
            for j in range(max(0, i-max_len), i):
                if s[j:i] in dictionary:
                    dp[i] = min(dp[j], dp[i])
        return dp[-1]


