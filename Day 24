class Solution:
	# @param A : list of integers
	# @return a list of integers
	def prevSmaller(self, A):
        s = list()
        res = []
        
        for i in range(len(A)):
            while (len(s) > 0 and s[-1] >= A[i]):
                s.pop()
            
            if (len(s) == 0):
                res.append(-1)
            else:
                res.append(s[-1])
            s.append(A[i])
        return res
        
