class Solution:
    #Function to find maximum number of consecutive steps 
    #to gain an increase in altitude with each step.
    def maxStep(self, arr):
        #Your code here
        count = res = 0
        for i in range(1, len(arr)):
            if arr[i] > arr[i-1]:
                count += 1
            else:
                count = 0
            res = max(res, count)
        return


