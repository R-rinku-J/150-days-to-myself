class Solution:
    # @param A : string
    # @return an integer
    def solve(self, A):
        count = 0
        temp = []
        for i in A:
            if i == '(':
                temp.append(i)
            else:
                if len(temp) != 0:
                    temp.pop()
                else:
                    count += 1
        count += len(temp)
        return count
