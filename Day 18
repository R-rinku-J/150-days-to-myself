#User function Template for python3
class Solution:
    def merge(self, root1, root2):
        # code here
        container = []
        
        def f(node):
            nonlocal container
            if not node:
                return
            
            f(node.left)
            container.append(node.data)
            f(node.right)
        
        f(root1)
        f(root2)
        
        container.sort()
        return container#User function Template for python3
class Solution:
    def merge(self, root1, root2):
        # code here
        container = []
        def f(node):
            nonlocal container
            if not node:
                return
            f(node.left)
            container.append(node.data)
            f(node.right)
        f(root1)
        f(root2)
        container.sort()
        return container


