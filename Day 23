from typing import List
class Solution:
    def pairsum(self, arr : List[int]) -> int:
        # code here
        num1, num2 = 0, 0
        for a in arr:
            if a > num1:
                num2 = num1
                num1 = a
            elif a > num2:
                num2 = a
        return num1 + num2
