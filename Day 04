class Solution:
    def MedianOfArrays(self, array1, array2):
            #code here
            a3=sorted(array1+array2)
            n=len(a3)
            m=n//2
            if n%2!=0:
                return a3[m]
            else:
                g=a3[m]+a3[m-1]
                c=(g/2)
            return int(c) if (a3[m]+a3[m-1])%2==0 else c


