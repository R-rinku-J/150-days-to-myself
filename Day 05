from typing import List
class Solution:
    def stockBuyAndSell(self, n : int, pricess : List[int]) -> int:
        # code here
        profit = 0
        for i in range(1, n):
            if prices[i] > prices[i - 1]:
                profit += prices[i] - prices[i-1]
        return profit
