# cook your dish here
T = int(input())
for i in range(T):
    N, K = map(int, input().split(' '))
    A = list(map(int, input().split(' ')))
    remaining = 0
    days = 0
    for i in range(len(A)):
        if A[i]+remaining < K:
            print('NO', i+1)
            break
        else:
            remaining = (A[i]+remaining) - K
            days += 1
    if days == len(A):
        print('YES')


