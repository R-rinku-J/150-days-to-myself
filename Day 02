class Solution:
    def longestSubarray(self, nums: List[int]) -> int:
        maxi = max(nums)
        curr = longest = 0
        for i in nums:
            if i == maxi:
                curr += 1
                longest = max(longest,  curr)
            else:
                curr = 0
        return longest


