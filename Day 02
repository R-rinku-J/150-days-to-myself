#User function Template for python3
class Solution:
    def rearrange(self,arr):
        # code here
        positive = []
        negative = []
        for i in arr:
            if i >= 0:
                positive.append(i)
            else:
                negative.append(i)
        result = []
        p, n = 0, 0 
        length_pos = len(positive)
        length_neg = len(negative)
        while p < length_pos and n < length_neg:
            result.append(positive[p])
            result.append(negative[n])
            p += 1
            n += 1
        while p < length_pos:
            result.append(positive[p])
            p += 1
        while n < length_neg:
            result.append(negative[n])
            n += 1
        for i in range(len(arr)):
            arr[i] = result[i]


ğˆğ§ğ­ğğ«ğ¯ğ¢ğğ°ğğ¢ğ­ : ğŒğğ«ğ ğ ğ“ğ°ğ¨ ğ’ğ¨ğ«ğ­ğğ ğ‹ğ¢ğ¬ğ­ğ¬ ğˆğˆ
class Solution:
	# @param A : list of integers
	# @param B : list of integers
	def merge(self, A, B):
        A.extend(B)
        return A.sort()


ğ‚ğ¨ğğğ…ğ¨ğ«ğœğğ¬ : ğŸğŸğŸğŸğ - ğ’ğğšğ­ğ¢ğ§ğ  ğ¢ğ§ ğš ğğ®ğ¬
t = int(input())
for _ in range(t):
    n = int(input())
    x = int(input())
    left, right = x - 1, x + 1
    rule = True
    for p in range(1, n):
        x = int(input())
        if x == left:
            left -= 1
        elif x == right:
            right += 1
        else:
            rule = False
    if rule:
        print("YES")
    else:
        print("NO")


