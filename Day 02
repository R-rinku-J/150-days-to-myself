# cook your dish here
t = int(input())
for i in range(t):
    d_dsa, d_toc, d_dm = map(int, input().split())
    s_dsa, s_toc, s_dm = map(int, input().split())
    total_d = d_dsa + d_toc + d_dm
    total_s = s_dsa + s_toc + s_dm
    if total_d > total_s:
        print('Dragon')
    elif total_s > total_d:
        print('Sloth')
    else:
        if d_dsa > s_dsa:
            print('Dragon')
        elif s_dsa > d_dsa:
            print('Sloth')
        else:
            if d_toc > s_toc:
                print('Dragon')
            elif s_toc > d_toc:
                print('Sloth')
            else:
                print('Tie')


